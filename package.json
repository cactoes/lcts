{
  "name": "lcts",
  "version": "1.0.0",
  "description": "lcts",
  "main": "dst/index.js",
  "scripts": {
    "tsc:build": "tsc",
    "start": "electron-forge start",
    "clean": "del /S dst\\*",
    "test": "tsc && electron-forge start",
    "package:win": "tsc && electron-builder --dir"
  },
  "build": {
    "appId": "lcts",
    "extraResources": [
      {
        "from": "./resources/data/",
        "to": "data"
      }
    ],
    "icon": "images/icon.ico",
    "files": [
      "!src",
      "!release*",
      "!images",
      "!.gitignore",
      "!package-lock.json",
      "!README.md",
      "!test.temp.js",
      "!TODO.md",
      "!tsconfig.json"
    ],
    "directories": {
      "output": "release"
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "lcts"
          }
        }
      ]
    }
  },
  "keywords": [],
  "author": "cactoes",
  "license": "GNU GPLv3",
  "repository": {
    "url": "https://github.com/cactoes/lcts"
  },
  "dependencies": {
    "@nut-tree/nut-js": "^2.1.1",
    "electron-overlay-window": "^2.0.1",
    "ffi-napi": "^4.0.3",
    "gkm": "^0.2.0",
    "lcinterface": "^4.0.0",
    "node-fetch": "^2.6.1",
    "openurl": "^1.1.1",
    "puppeteer": "^16.1.0",
    "puppeteer-core": "^16.1.0",
    "puppeteer-in-electron": "^3.0.5",
    "ref-napi": "^3.0.3",
    "ref-struct-di": "^1.1.1",
    "ref-wchar-napi": "^1.0.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@types/node": "^18.0.0",
    "@types/node-fetch": "^2.6.2",
    "electron": "^19.0.5",
    "electron-builder": "^23.1.0"
  }
}
